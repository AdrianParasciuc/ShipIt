@using AccesaChallengePortal.Models
@model IEnumerable<ChallengeModel>
@{
    ViewBag.Title = "Home Page";
}

<p>
    @Html.ActionLink("Create new challenge", "Create")
</p>

<table class="table">
    <tr>
        <th>
            Title
        </th>
        <th>
        </th>
    </tr>
    @foreach (var c in Model)
    {
        <tr>
            <td>
                @c.Title
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = c.Id })
                @Html.ActionLink("Delete", "Delete", new { id = c.Id })
                @Html.ActionLink("Detail", "Detail", new { id = c.Id })
            </td>
        </tr>
    }
</table>